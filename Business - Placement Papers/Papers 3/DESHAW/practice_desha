#include<stdio.h>
main()
{
int arr[3][3] = {1,2,3,
		 4,5,6,
		 7,8,9};
int i,j;
for (j=2;j>=0;j--){
	for(i=2;i>=0;i--){
	printf("\n%d",*(*(arr+i)+j));
	printf("\n TATATATA");
	}
	}
}

main()
	{
		int i = 5, j=10;
		abc(&i,&j);
		printf("%d..%d",i,j);
	}

	abc(int *i, int *j)
	{
		*i = *i + *j;
		*j = *i - *j;
		*i = *i - *j;
	}

