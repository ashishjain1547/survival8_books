#include<stdio.h>
#define PR(a)   printf("%d\t",(int) (a));
#define PRINT(a,b,c) PR(a);PR(b);PR(c);
#define MAX(a,b) (a<b?b:a)
main(){
	int x = 1,y = 2;
		PRINT(MAX(x++,y),x,y);
			PRINT(MAX(x++,y),x,y);
			}

